---
interface Props {
  title: string
}

const { title } = Astro.props
---

<script src="@/scripts/app.ts"></script>

<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <title>{title}</title>
    <link
      rel="icon"
      type="image/svg+xml"
      href="/favicon.svg"
    />
  </head>
  <body>
    <slot />
  </body>
</html>
